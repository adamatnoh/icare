<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>appointmentController</servlet-name>
        <servlet-class>controller.appointmentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileController</servlet-name>
        <servlet-class>controller.profileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileControllers</servlet-name>
        <servlet-class>controller.profileControllers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrationController</servlet-name>
        <servlet-class>controller.registrationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doctorController</servlet-name>
        <servlet-class>controller.doctorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reportController</servlet-name>
        <servlet-class>controller.reportController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>appointmentController</servlet-name>
        <url-pattern>/appointmentController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileController</servlet-name>
        <url-pattern>/profileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileControllers</servlet-name>
        <url-pattern>/profileControllers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationController</servlet-name>
        <url-pattern>/registrationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doctorController</servlet-name>
        <url-pattern>/doctorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reportController</servlet-name>
        <url-pattern>/reportController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
